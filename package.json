    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "puppeteer": "10.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.19.0",
    "ts-jest": "26.5.6",
    "typescript": "4.3.4"
  }
