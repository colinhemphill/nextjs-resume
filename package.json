    "@radix-ui/colors": "0.1.8",
    "@react-pdf/renderer": "3.1.7",
    "@vercel/og": "0.4.0",
    "front-matter": "4.0.2",
    "indefinite": "2.4.2",
    "marked": "4.2.12",
    "next": "13.2.4",
    "react": "18.2.0",
