{
  "name": "next-resume-generator",
  "version": "0.1.0",
  "author": {
    "name": "Colin Hemphill",
    "email": "colin@colinhemphill.com",
    "url": "https://colinhemphill.com"
  },
  "keywords": [
    "resume",
    "generator",
    "nextjs",
    "pdf"
  ],
  "homepage": "https://github.com/colinhemphill/nextjs-resume-generator",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write 'src/**/*.{json,js,ts,tsx}'",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint ./src --ext js --ext ts --ext tsx --fix",
    "lint:styles": "stylelint 'src/styles/**/*.scss' --fix",
    "start": "next start",
    "types": "tsc --pretty --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "npm run types",
      "npm run lint:js",
      "npm run lint:styles",
      "npm run format"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/free-regular-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.13",
    "bootstrap": "4.5.3",
    "chrome-aws-lambda": "5.5.0",
    "classnames": "2.2.6",
    "next": "10.0.3",
    "next-offline": "5.0.3",
    "prismic-javascript": "3.0.2",
    "prismic-reactjs": "1.3.3",
    "puppeteer": "5.5.0",
    "puppeteer-core": "5.5.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "1.29.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "10.0.3",
    "@types/node": "14.14.10",
    "@types/puppeteer": "5.4.1",
    "@types/react": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "stylelint": "13.8.0",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.18.0",
    "typescript": "4.1.2"
  }
}