{
  "name": "next-resume-generator",
  "version": "2.0.6",
  "author": {
    "name": "Colin Hemphill",
    "email": "colin@colinhemphill.com",
    "url": "https://colinhemphill.com"
  },
  "keywords": [
    "resume",
    "generator",
    "nextjs",
    "pdf"
  ],
  "homepage": "https://github.com/colinhemphill/nextjs-resume-generator",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "copy": "copyfiles --flat edit-me/config/**/*.{png,ico,json} public",
    "dev": "next dev",
    "format": "prettier --write 'src/**/*.{json,js,ts,tsx}'",
    "lint": "next lint",
    "prepare": "husky install && npm run copy",
    "start": "next start",
    "types": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@heroicons/react": "2.0.13",
    "@next/font": "13.1.0",
    "@radix-ui/colors": "0.1.8",
    "@react-pdf/renderer": "3.0.1",
    "@strum/react": "1.2.1",
    "@vanilla-extract/css": "1.9.2",
    "@vanilla-extract/css-utils": "0.1.3",
    "@vanilla-extract/dynamic": "2.0.3",
    "@vanilla-extract/next-plugin": "2.1.1",
    "@vanilla-extract/recipes": "0.3.0",
    "@vanilla-extract/sprinkles": "1.5.1",
    "front-matter": "4.0.2",
    "indefinite": "2.4.2",
    "marked": "4.2.4",
    "next": "13.1.0",
    "next-compose-plugins": "2.2.1",
    "normalize.css": "8.0.1",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-pdf-html": "1.1.15",
    "tiny-invariant": "1.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.1.0",
    "@tailwindcss/typography": "0.5.8",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "autoprefixer": "10.4.13",
    "copyfiles": "2.4.1",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "next-router-mock": "0.8.0",
    "postcss": "8.4.20",
    "prettier": "2.8.1",
    "prettier-plugin-tailwindcss": "0.2.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4",
    "webpack": "5.75.0"
  },
  "overrides": {
    "@react-pdf/renderer": {
      "react": "^18.0.0"
    }
  }
