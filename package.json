    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@next/bundle-analyzer": "11.0.0",
    "bootstrap": "5.0.1",
    "chrome-aws-lambda": "10.0.0",
    "classnames": "2.3.1",
    "contentful": "8.4.2",
    "indefinite": "2.4.1",
    "next": "11.0.0",
    "next-compose-plugins": "2.2.1",
    "next-pwa": "5.2.21",
