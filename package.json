    "@react-pdf/renderer": "4.3.1",
    "@strum/colors": "0.1.8",
    "babel-plugin-react-compiler": "1.0.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "contentlayer2": "0.5.8",
    "eslint": "9.39.2",
    "eslint-config-next": "16.0.10",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-unicorn": "62.0.0",
    "next": "16.0.10",
    "next-contentlayer2": "0.5.8",
    "next-themes": "0.4.6",
    "npm-run-all2": "8.0.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-pdf-html": "2.1.4",
    "tailwind-merge": "3.4.0"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "20.0.11",
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "25.0.1",
    "@types/react": "19.2.7",
    "bun-types": "1.3.4",
    "jsdom": "27.3.0",
    "minimatch": "10.1.1",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-sort-json": "4.1.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "syncpack": "13.0.4",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  },
  "homepage": "https://github.com/colinhemphill/nextjs-resume",
  "keywords": [
    "generator",
    "nextjs",
    "pdf",
    "resume",
    "tailwind"
  ],
  "license": "MIT",
  "overrides": {
    "@types/react": "19.2.2"
  },
  "scripts": {
    "build": "contentlayer2 build && next build",
    "build:content": "contentlayer2 build",
    "dev": "run-p dev:*",
    "dev:contentlayer": "contentlayer2 dev",
    "dev:next": "next dev --turbopack",
    "format": "prettier --write 'src/**/*.{json,js,ts,tsx}' && syncpack format",
    "lint": "eslint --fix . && syncpack format && syncpack lint",
    "start": "next start",
    "sync": "syncpack fix-mismatches",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:update": "bun test --update-snapshots",
    "test:watch": "bun test --watch",
    "types": "tsc -p tsconfig.json --noEmit",
    "update": "syncpack update",
    "yeet": "bunx rimraf bun.lockb && bunx rimraf **/{dist,node_modules,.turbo,.next,.vercel,.netlify,__snapshots__}/ --glob && bunx rimraf **/tsconfig.tsbuildinfo --glob"
  },
  "type": "module"
}