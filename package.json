    "prismic-reactjs": "1.3.4",
    "puppeteer-core": "10.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.35.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
